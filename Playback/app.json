{
  "expo": {
    "description": "A React Native video-game backlog app.",
    "owner": "Jubstacheit",
    "privacy": "unlisted",
    "version": "0.1.0",
    "backgroundColor": "#ffffff",
    "userInterfaceStyle": "automatic",
    "primaryColor": "#B8336A",
    "icon": "./assets/icon.png",
    "notification": {
      "icon": "./assets/push-icon.png",
      "color": "#B8336A",
      "androidMode": "collapse",
      "androidCollapsedTitle": "#{unread_notifications} new notifications"
    },
    "androidStatusBar": {
      "barStyle": "dark-content",
      "backgroundColor": "#E5FCFF"
    },
    "platforms": [
      "android",
      "web"
    ],
    "githubUrl": "https://github.com/Jubstacheit/Playback",
    "orientation": "portrait",
    "scheme": "acme",
    "web": {
      "bundler": "metro",
      "favicon": "./assets/favicon.png",
      "name": "Playback",
      "shortName": "Playback",
      "lang": "en-US",
      "themeColor": "#000000",
      "description": "A React Native video-game backlog app.",
      "display": "browser",
      "orientation": "landscape",
      "backgroundColor": "#E5FCFF",
      "splash": {
        "image": "./assets/splash.png",
        "resizeMode": "contain",
        "backgroundColor": "#E5FCFF"
      }
    },
    "plugins": [
      "expo-router"
    ],
    "name": "Playback",
    "slug": "Playback",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#E5FCFF"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#E5FCFF"
      }
    }
  }
}
